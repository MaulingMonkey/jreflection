{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "classlist | Debug MSVC", "type": "cppvsdbg", "request": "launch",
            "preLaunchTask": "classlist | cargo build",
            "cwd": "${workspaceRoot}/target/debug",
            "program": "examples/classlist",
            "internalConsoleOptions": "openOnSessionStart", "externalConsole": false,
        },
        {
            "name": "classlist | Release MSVC", "type": "cppvsdbg", "request": "launch",
            "preLaunchTask": "classlist | cargo build --release",
            "cwd": "${workspaceRoot}/target/release",
            "program": "examples/classlist",
            "internalConsoleOptions": "openOnSessionStart", "externalConsole": false,
        },
    ]
}
